<VLDocking version="3.0.0">
	<DockingDesktop>
		<DockingPanel>
			<Split orientation="0">
				<Split orientation="1">
					<Dockable>
						<Key dockName="personMasterViewDescriptor" dockLabel="personMasterViewDescriptor" />
					</Dockable>
					<Dockable>
						<Key dockName="personSearchViewDescriptor" dockLabel="personSearchViewDescriptor" />
