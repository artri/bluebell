	<build>
		<extensions>
			<extension>
				<!--
					(JAF) 20091228: use a customized Wagon for SVN 
				 -->
				<groupId>org.jvnet.wagon-svn</groupId>
				<artifactId>org.bluebell.wagon-svn</artifactId>
